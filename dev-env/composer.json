{
    "minimum-stability": "dev",
    "require": {
        "slim/slim": "^4.14",
        "slim/psr7": "^1.7",
        "php-di/php-di": "^7.0",
        "buzzingpixel/php-queue": "dev-main",
        "filp/whoops": "^2.16",
        "symfony/var-dumper": "6.4.x-dev",
        "buzzingpixel/symfony-var-dumper-decorator": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "/php-queue",
            "options": {
                "symlink": true
            }
        }
    ]
}
